{
  "name": "playground-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "playground-service": "dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:all": "tsc",
    "db:push:dev": "dotenv -e ./.env/.env.development -- prisma migrate dev",
    "db:push:test": "dotenv -e ./.env/.env.test -- prisma migrate dev",
    "interface:list:gen": "cross-env DEBUG=@lsby:* lsby-net-core-gen-list ./tsconfig.json ./src/interface ./src/interface/interface-list.ts",
    "other:format": "prettier --write .",
    "other:lint": "eslint . --fix",
    "other:pub": "npm run other:check && npm run build:all && bumpp && pnpm -r publish --access public",
    "start:dev": "npm run interface:list:gen && cross-env MODE=development DEBUG=@lsby:*,playground-service:* tsx ./src/index.ts",
    "test:gen": "cross-env DEBUG=@lsby:* lsby-net-core-gen-test ./tsconfig.json ./src/interface ./test/unit-test.test.ts",
    "test:unit": "npm run type:check && npm run interface:list:gen && cross-env MODE=test DEBUG=@lsby:*,playground-service:* tsx script/test/run-test.ts",
    "test:unit:coverage": "npm run type:check && npm run interface:list:gen && cross-env MODE=test DEBUG=@lsby:*,playground-service:* tsx script/test/run-test-coverage.ts && open-cli ./coverage/index.html",
    "type:check": "tsc --noEmit",
    "type:check:watch": "tsc --noEmit -w",
    "type:gen": "cross-env DEBUG=@lsby:* lsby-net-core-gen-type ./tsconfig.json ./src/interface ./dist/type.ts"
  },
  "dependencies": {
    "@lsby/net-core": "0.0.14",
    "@lsby/ts-fp-data": "^0.0.6",
    "axios": "^1.7.2",
    "better-sqlite3": "^11.1.2",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express-fileupload": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/debug": "^4.1.12",
    "@types/express-fileupload": "^1.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^2.0.2",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "open-cli": "^8.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "prisma": "^5.16.1",
    "prisma-kysely": "^1.7.1",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "vitest": "^2.0.2"
  }
}
