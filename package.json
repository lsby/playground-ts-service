{
  "name": "@lsby/playground-ts-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "lsby-playground-ts-service": "dist/src/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "_build:all": "npm run _build:web && npm run _build:service",
    "_build:service": "tsc && tsc-alias -f",
    "_build:web": "npm run _clear:web && parcel build --no-cache --no-source-maps src/web/page/**/*.html --dist-dir dist/web",
    "_check:all": "npm run _check:format && npm run _check:lint && npm run _check:type",
    "_check:format": "prettier --write .",
    "_check:lint": "eslint . --fix",
    "_check:type": "tsc --noEmit",
    "_clear:web": "rm -rf dist/web",
    "_gen:all": "npm run _gen:api:list && npm run _gen:api:type && npm run _gen:web:index",
    "_gen:api:list": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* lsby-net-core-gen-api-list ./tsconfig.json ./src/interface ./src/interface/interface-list.ts",
    "_gen:api:type": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* lsby-net-core-gen-api-type ./tsconfig.json ./src/interface ./src/types/interface-type.ts",
    "_gen:web:index": "tsx script/gen/gen-web-index.ts",
    "capacitor:add:android": "cap add android",
    "capacitor:copy": "cap copy",
    "capacitor:init": "cap init",
    "capacitor:open:android": "cap open android",
    "db:push:dev": "dotenv -e ./.env/.env.development -- prisma migrate dev && npm run _check:lint",
    "db:push:prod": "dotenv -e ./.env/.env.production -- prisma migrate dev && npm run _check:lint",
    "db:push:test": "dotenv -e ./.env/.env.test -- prisma migrate dev && npm run _check:lint",
    "prepare": "husky",
    "public:docker": "tsx script/docker/make-docker.ts",
    "public:npm": "npm publish --access public",
    "release": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* npm run _gen:all && npm run _check:all && npm run _build:all && bumpp",
    "run:service:debug": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.development tsx watch --exclude ./src/types/**/* --exclude ./src/interface/interface-list.ts --inspect ./src/server.ts",
    "run:service:dev": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.development node ./dist/src/server.js",
    "run:service:prod": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.production node ./dist/src/server.js",
    "run:service:test": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.test node ./dist/src/server.js",
    "run:web:debug": "bash -c \"while true; do npm run _clear:web && parcel --no-cache --dist-dir dist/web --watch-for-stdin --port 4000 --hmr-port 4001 src/web/page/**/*.html; echo 崩了！重启！; sleep 1; done\"",
    "test:unit": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test.ts",
    "test:unit:coverage": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test-coverage.ts && open-cli ./coverage/index.html",
    "watch:check:type": "tsc --noEmit -w",
    "watch:gen:api:list": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/interface/ \"npm run _gen:api:list\"",
    "watch:gen:api:type": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/interface/ \"npm run _gen:api:type\"",
    "watch:gen:web:index": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/web/components/ \"npm run _gen:web:index\""
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@lsby/net-core": "0.6.7-beta.19",
    "@lsby/net-core-file-upload": "^0.1.9",
    "@lsby/net-core-jwt": "^0.1.10",
    "@lsby/net-core-kysely": "^2.0.3",
    "@lsby/ts-cron": "^0.0.7",
    "@lsby/ts-env": "^0.0.12",
    "@lsby/ts-fp-data": "^0.2.5",
    "@lsby/ts-global": "^0.0.7",
    "@lsby/ts-kysely": "^0.2.3",
    "@lsby/ts-log": "^0.1.2",
    "@lsby/ts-post-extend": "^0.0.19",
    "@lsby/ts-web-component": "^0.0.1",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.9.1",
    "kysely": "^0.27.6",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@lsby/eslint-config": "^0.2.21",
    "@lsby/eslint-plugin": "^0.0.11",
    "@types/better-sqlite3": "^7.6.12",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.6.1",
    "@types/pg": "^8.15.2",
    "@types/stream-to-blob": "^2.0.0",
    "@vitest/coverage-v8": "^2.1.1",
    "bumpp": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.0",
    "lint-staged": "^15.5.0",
    "node-watch": "^0.7.4",
    "open-cli": "^8.0.0",
    "parcel": "^2.14.2",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "prisma": "^5.19.1",
    "prisma-kysely": "^1.8.0",
    "stream-to-blob": "^2.0.1",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "<5.6.0",
    "typescript-eslint": "8.7.0",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
