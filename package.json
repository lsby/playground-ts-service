{
  "name": "playground-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "playground-service": "dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:all": "tsc",
    "check:all": "npm run check:format && npm run check:lint && npm run check:type:check",
    "check:format": "prettier --write .",
    "check:lint": "eslint . --fix",
    "check:type:check": "tsc --noEmit",
    "check:type:check:watch": "tsc --noEmit -w",
    "db:push:dev": "dotenv -e ./.env/.env.development -- prisma migrate dev",
    "db:push:test": "dotenv -e ./.env/.env.test -- prisma migrate dev",
    "gen:list": "cross-env DEBUG=@lsby:* lsby-net-core-gen-list ./tsconfig.json ./src/interface ./src/interface/interface-list.ts",
    "gen:type": "cross-env DEBUG=@lsby:* lsby-net-core-gen-type ./tsconfig.json ./src/interface ./out/type.ts",
    "pub:public": "npm run check:all && npm run build:all && bumpp && npm publish --access public",
    "run:dev": "npm run gen:list && cross-env ENV_FILE_PATH=./.env/.env.development DEBUG=@lsby:*,playground-service:* tsx ./src/index.ts",
    "run:prod": "npm run check:all && npm run gen:list && cross-env ENV_FILE_PATH=./.env/.env.development DEBUG=@lsby:*,playground-service:* tsx ./src/index.ts",
    "test:gen": "cross-env DEBUG=@lsby:* lsby-net-core-gen-test ./tsconfig.json ./src/interface ./test/unit-test.test.ts",
    "test:unit": "npm run check:all && npm run gen:list && cross-env ENV_FILE_PATH=./.env/.env.test DEBUG=@lsby:*,playground-service:* tsx script/test/run-test.ts",
    "test:unit:coverage": "npm run check:all && npm run gen:list && cross-env ENV_FILE_PATH=./.env/.env.test DEBUG=@lsby:*,playground-service:* tsx script/test/run-test-coverage.ts && open-cli ./coverage/index.html"
  },
  "dependencies": {
    "@lsby/net-core": "0.0.20-beta.4",
    "@lsby/ts-fp-data": "^0.0.9",
    "@lsby/ts-log": "^0.0.1",
    "axios": "^1.7.2",
    "better-sqlite3": "^11.1.2",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express-fileupload": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "node-schedule": "^2.1.1",
    "stream-to-blob": "^2.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@lsby/eslint-config": "^0.0.2",
    "@types/better-sqlite3": "^7.6.11",
    "@types/debug": "^4.1.12",
    "@types/express-fileupload": "^1.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.10",
    "@types/node-schedule": "^2.1.7",
    "@types/stream-to-blob": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.2",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "open-cli": "^8.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "prisma": "^5.16.1",
    "prisma-kysely": "^1.7.1",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "vitest": "^2.0.2"
  }
}
