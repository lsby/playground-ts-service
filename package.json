{
  "name": "playground-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js"
  },
  "bin": {
    "playground-ts-bin": "dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:all": "tsc",
    "other:clean": "rm -rf dist",
    "other:format": "prettier --write .",
    "other:lint": "eslint . --fix",
    "other:pub": "npm run other:typecheck && npm run build:all && bumpp && pnpm -r publish --access public",
    "other:typecheck": "tsc --noEmit",
    "other:typecheck:watch": "tsc --noEmit -w",
    "start:dev": "cross-env DEBUG=playground-service:* dotenv -e ./.env/.env.development tsx ./src/index.ts"
  },
  "dependencies": {
    "@lsby/net-core": "^0.0.4",
    "@lsby/ts-fp-data": "^0.0.6",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  }
}
