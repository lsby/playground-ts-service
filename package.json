{
  "name": "@lsby/playground-ts-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/src/electron.js",
  "bin": {
    "lsby-playground-ts-service": "dist/src/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "_build:all": "npm run _build:service && npm run _build:web",
    "_build:service": "tsc && tsc-alias -f",
    "_build:web": "npm run _clean:web && parcel build --no-cache --no-source-maps src/web/page/**/*.html --dist-dir dist/src/web",
    "_build:web:no-scope-hoist": "npm run _clean:web && parcel build --no-cache --no-source-maps --no-scope-hoist src/web/page/**/*.html --dist-dir dist/src/web",
    "_check:all": "npm run _check:format && npm run _check:lint && npm run _check:type",
    "_check:format": "prettier --write .",
    "_check:lint": "eslint . --fix",
    "_check:type": "tsc --noEmit",
    "_clean:all": "npm run _clean:web && npm run _clean:electron",
    "_clean:electron": "tsx script/run/clean-electron.ts",
    "_clean:web": "tsx script/run/clean-web.ts",
    "_gen:all": "npm run _gen:api:list && npm run _gen:api:type && npm run _gen:web:index && npm run _gen:meta-info",
    "_gen:api:list": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* lsby-net-core-gen-api-list ./tsconfig.json ./src/interface ./src/interface/interface-list.ts",
    "_gen:api:type": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* lsby-net-core-gen-api-type ./tsconfig.json ./src/interface ./src/types/interface-type.ts",
    "_gen:meta-info": "tsx script/gen/write-meta-info.ts",
    "_gen:web:index": "tsx script/gen/gen-web-index.ts",
    "capacitor:add:android": "cap add android",
    "capacitor:init": "cap init",
    "capacitor:open:android": "cap open android",
    "capacitor:sync:android": "cap sync android",
    "db:push:dev": "dotenv -e ./.env/.env.development -- prisma migrate dev && dotenv -e ./.env/.env.development -- prisma generate",
    "db:push:prod": "dotenv -e ./.env/.env.production -- prisma migrate dev && dotenv -e ./.env/.env.production -- prisma generate",
    "db:push:test": "dotenv -e ./.env/.env.test -- prisma migrate dev && dotenv -e ./.env/.env.test -- prisma generate",
    "prepare": "husky",
    "public:docker": "tsx script/docker/make-docker.ts",
    "public:electron": "npm run _check:all && npm run _clean:all && npm run _build:all && electron-builder && tsx script/run/after-electron.ts",
    "public:npm": "npm publish --access public",
    "release": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* npm run _gen:all && npm run _check:all && bumpp --no-commit --no-tag --no-push && tsx script/release/release.ts",
    "run:electron:dev": "chcp 65001 && cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.development nodemon --watch src --ext ts --ignore src/types/**/* --ignore src/interface/interface-list.ts --ignore src/web/**/* --exec \"electron -r tsx src/electron.ts\"",
    "run:electron:prod": "chcp 65001 && cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.production electron -r tsx dist/src/electron.js",
    "run:service:dev": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* RUN_MODE=dist ENV_FILE_PATH=./.env/.env.development node ./dist/src/server.js",
    "run:service:prod": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* RUN_MODE=dist ENV_FILE_PATH=./.env/.env.production node ./dist/src/server.js",
    "run:service:watch": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* RUN_MODE=tsx ENV_FILE_PATH=./.env/.env.development tsx watch --exclude ./src/types/**/* --exclude ./src/interface/interface-list.ts --inspect ./src/server.ts",
    "run:web:dev": "tsx script/run/run-web.ts",
    "test:unit": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test.ts",
    "test:unit:coverage": "cross-env DEBUG=@lsby:*,@lsby:playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test-coverage.ts && open-cli ./coverage/index.html",
    "watch:check:lint": "tsx script/watch/watch.ts src \"eslint .\"",
    "watch:check:type": "tsc --noEmit -w",
    "watch:gen:api:list": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/interface/ \"npm run _gen:api:list\"",
    "watch:gen:api:type": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/interface/ \"npm run _gen:api:type\"",
    "watch:gen:web:index": "cross-env DEBUG=@lsby:* tsx script/watch/watch.ts ./src/web/components/ \"npm run _gen:web:index\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.0",
    "@capacitor/dialog": "^7.0.2",
    "@lsby/net-core": "^0.13.10",
    "@lsby/net-core-jwt": "^0.5.1",
    "@lsby/net-core-kysely": "^0.5.0",
    "@lsby/ts-env": "^0.0.13",
    "@lsby/ts-fp-data": "^0.2.7",
    "@lsby/ts-http-extend": "^0.3.3",
    "@lsby/ts-kysely": "^0.2.9",
    "@lsby/ts-log": "^0.3.1",
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "express": "^5.2.1",
    "kysely": "^0.28.9",
    "kysely-wasm": "^1.2.1",
    "node-schedule": "^2.1.1",
    "node-sqlite3-wasm": "^0.8.52",
    "prisma": "^7.2.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@lsby/eslint-config": "^0.5.3",
    "@lsby/eslint-plugin": "^0.2.1",
    "@parcel/config-default": "^2.16.3",
    "@parcel/transformer-css": "^2.16.3",
    "@parcel/transformer-inline-string": "2.16.3",
    "@parcel/transformer-postcss": "^2.16.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcrypt": "^6.0.0",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.6",
    "@types/node-schedule": "^2.1.8",
    "@types/stream-to-blob": "^2.0.0",
    "@vitest/coverage-v8": "^2.1.1",
    "bumpp": "^10.3.1",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "husky": "^9.1.7",
    "inquirer": "^12.9.4",
    "lint-staged": "^15.5.0",
    "node-watch": "^0.7.4",
    "nodemon": "^3.1.11",
    "open-cli": "^8.0.0",
    "parcel": "^2.16.3",
    "postcss": "^8.2.1",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.20",
    "prisma-kysely": "^1.8.0",
    "process": "^0.11.10",
    "stream-to-blob": "^2.0.1",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "util": "^0.12.5",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "build": {
    "appId": "lsby.playground.ts.service",
    "productName": "lsby-playground-ts-service",
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "package"
    },
    "win": {
      "signAndEditExecutable": false,
      "target": [
        "dir"
      ]
    }
  },
  "targets": {
    "main": false
  }
}
