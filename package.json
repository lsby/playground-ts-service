{
  "name": "playground-ts-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "playground-ts-service": "dist/src/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "_build:all": "npm run _build:web && npm run _build:service",
    "_build:service": "tsc && tsc-alias -f",
    "_build:web": "vite build -c vite.config/production.js",
    "_check:all": "npm run _check:format && npm run _check:lint && npm run _check:type",
    "_check:format": "prettier --write .",
    "_check:lint": "eslint . --fix",
    "_check:type": "tsc --noEmit",
    "_gen:all": "npm run _gen:api:list && npm run _gen:table:list && npm run _gen:api:type && npm run _gen:table:type",
    "_gen:api:list": "cross-env DEBUG=@lsby:*,playground-ts-service:* lsby-net-core-gen-api-list ./tsconfig.json ./src/interface-api ./src/interface-api/interface-api-list.ts",
    "_gen:api:type": "cross-env DEBUG=@lsby:*,playground-ts-service:* lsby-net-core-gen-api-type ./tsconfig.json ./src/interface-api ./src/types/interface-api-type.ts",
    "_gen:table:list": "cross-env DEBUG=@lsby:*,playground-ts-service:* lsby-net-core-gen-table-list ./tsconfig.json ./src/interface-table ./src/interface-table/interface-table-list.ts",
    "_gen:table:type": "cross-env DEBUG=@lsby:*,playground-ts-service:* lsby-net-core-gen-table-type ./tsconfig.json ./src/interface-table ./src/types/interface-table-type.ts",
    "db:push:dev": "dotenv -e ./.env/.env.development -- prisma migrate dev",
    "db:push:prod": "dotenv -e ./.env/.env.production -- prisma migrate dev",
    "db:push:test": "dotenv -e ./.env/.env.test -- prisma migrate dev",
    "prepare": "husky",
    "public:docker": "tsx script/docker/make-docker.ts",
    "public:npm": "npm publish --access public",
    "release": "cross-env DEBUG=@lsby:*,playground-ts-service:* npm run _gen:all && npm run _check:all && npm run _build:all && bumpp",
    "run:service:debug": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.development tsx --watch ./src/index.ts",
    "run:service:dev": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.development node ./dist/src/index.js",
    "run:service:prod": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.production node ./dist/src/index.js",
    "run:service:test": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.test node ./dist/src/index.js",
    "run:web:debug": "vite -c vite.config/development.js",
    "test:unit": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test.ts",
    "test:unit:coverage": "cross-env DEBUG=@lsby:*,playground-ts-service:* ENV_FILE_PATH=./.env/.env.test tsx script/test/run-test-coverage.ts && open-cli ./coverage/index.html",
    "watch:check:type": "tsc --noEmit -w",
    "watch:gen:api:list": "cross-env DEBUG=@lsby:* tsx script/gen/watch.ts ./src/interface-api/ \"npm run _gen:api:list\"",
    "watch:gen:api:type": "cross-env DEBUG=@lsby:* tsx script/gen/watch.ts ./src/interface-api/ \"npm run _gen:api:type\"",
    "watch:gen:table:list": "cross-env DEBUG=@lsby:* tsx script/gen/watch.ts ./src/interface-table/ \"npm run _gen:table:list\"",
    "watch:gen:table:type": "cross-env DEBUG=@lsby:* tsx script/gen/watch.ts ./src/interface-table/ \"npm run _gen:table:type\""
  },
  "dependencies": {
    "@lsby/net-core": "0.4.0-beta.12",
    "@lsby/net-core-file-upload": "^0.1.2",
    "@lsby/net-core-jwt": "^0.1.2",
    "@lsby/net-core-kysely": "0.0.2",
    "@lsby/ts-cron": "^0.0.6",
    "@lsby/ts-env": "^0.0.10",
    "@lsby/ts-fp-data": "^0.2.5",
    "@lsby/ts-global": "^0.0.5",
    "@lsby/ts-kysely": "^0.1.0",
    "@lsby/ts-log": "^0.1.1",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.3.0",
    "kysely": "^0.27.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@lsby/eslint-config": "^0.2.21",
    "@lsby/eslint-plugin": "^0.0.11",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/stream-to-blob": "^2.0.0",
    "@vitest/coverage-v8": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.5.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "husky": "^9.1.6",
    "inquirer": "^12.1.0",
    "lint-staged": "^15.2.10",
    "node-watch": "^0.7.4",
    "open-cli": "^8.0.0",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.2",
    "prisma": "^5.19.1",
    "prisma-kysely": "^1.8.0",
    "stream-to-blob": "^2.0.1",
    "tailwindcss": "^3.4.13",
    "ts-to-zod": "^3.13.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "<5.6.0",
    "typescript-eslint": "8.7.0",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.5.0"
}
