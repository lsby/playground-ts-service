{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "_监视生成接口类型",
      "type": "npm",
      "script": "gen:type:watch",
      "problemMatcher": [
        {
          "pattern": {
            "kind": "file",
            "regexp": "(错误: .*)",
            "file": 1,
            "message": 1
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^========生成开始========$",
            "endsPattern": "^========生成结束========$"
          }
        }
      ],
      "isBackground": true,
      "presentation": { "reveal": "never" },
      "group": { "kind": "build", "isDefault": true }
    },
    {
      "label": "_运行后端",
      "type": "npm",
      "script": "run:dev:service",
      "isBackground": true,
      "presentation": { "reveal": "never" },
      "group": { "kind": "build", "isDefault": true }
    },
    {
      "label": "_运行前端",
      "type": "npm",
      "script": "run:dev:web",
      "isBackground": true,
      "presentation": { "reveal": "never" },
      "group": { "kind": "build", "isDefault": true }
    },
    {
      "label": "_编译前端",
      "type": "npm",
      "script": "run:dev:web:watch",
      "isBackground": true,
      "presentation": { "reveal": "never" },
      "group": { "kind": "build", "isDefault": true }
    },
    {
      "label": "运行:合并运行",
      "dependsOrder": "parallel",
      "dependsOn": ["_监视生成接口类型", "_编译前端", "_运行后端"],
      "problemMatcher": []
    },
    {
      "label": "运行:独立运行",
      "dependsOrder": "parallel",
      "dependsOn": ["_监视生成接口类型", "_运行后端", "_运行前端"],
      "problemMatcher": []
    }
  ]
}
